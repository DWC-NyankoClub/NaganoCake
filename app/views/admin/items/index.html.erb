<div class ="container">
    <div class="low">
        <div>
            <h1>商品一覧</h1>
        </div>
        <div>
            <%= link_to "", new_admin_item_path, class:"fas fa-plus-circle fa-3x new_btn_color" %>
        </div>
    </div>
    <div class="low">
        <div class="col-md-10">
          <table>
              <thead>
                  <tr>商品ID</tr>
                  <tr>商品名</tr>
                  <tr>税抜価格</tr>
                  <tr>ジャンル</tr>
                  <tr>ステータス</tr>
              </thead>

            <% @items.each do |item|%>
              <tbody>
                  <td><%= item.id %></td>
                  <td><%= item.name %></td>
                  <td><%= item.price %></td>
                  <td><%= item.genre.name %></td>
                  <td><%= item.is_active %></td>
              </tbody>
             <% end %>
          </table>
        </div>
    </div>
</div>